    "font-size": "90%",
    "color": "contrast(@xf-successFeatureColor)",
    "background-color": "@xf-successFeatureColor",
    "border-radius": "@xf-borderRadiusSmall",
    "padding-top": "1px",
    "padding-right": "4px",
    "padding-bottom": "1px",
    "padding-left": "4px"
}</value>
  </style_property>
  <style_property property_name="zdessNegativeReactionScoreColor" group_name="zdessBasic" title="Negative reaction score color" property_type="value" value_type="color" display_order="30">
    <value>"@xf-errorColor"</value>
  </style_property>
  <style_property property_name="zdessPositiveReactionScoreColor" group_name="zdessBasic" title="Positive reaction score color" property_type="value" value_type="color" display_order="20">
    <value>"@xf-successColor"</value>
  </style_property>
  <style_property property_name="zdessShowPositiveNegativeReactions" group_name="zdessBasic" title="Show positive / negative reactions" description="It will show next to the total reaction score in the format (positive / negative)." property_type="value" value_type="boolean" display_order="0">
