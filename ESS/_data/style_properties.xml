<?xml version="1.0" encoding="utf-8"?>
<style_properties>
  <style_property property_name="zdessCenterOnlineIndicatorOnMediumResponsive" group_name="message" title="Center online indicator on medium responsive" property_type="value" value_type="boolean" value_group="user" display_order="700">
    <value>1</value>
  </style_property>
  <style_property property_name="zdessMessageThreadStarterIndicator" group_name="message" title="ZDESS: Message thread starter indicator" description="Usually a small token attached to the message block, the indicator that a message by thread starter is controlled here" property_type="css" display_order="20150" css_components="text,background,border,border_radius,padding,extra">
